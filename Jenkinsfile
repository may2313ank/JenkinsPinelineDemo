pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World from GIT'
            }
        }
        stage('Build') {
            steps {
                echo 'Building from GIT'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying from GIT'
            }
        }
        stage('Test') {
            steps {
                echo 'testing from GIT'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
